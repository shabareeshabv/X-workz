 class Jacket {
    String brand;
    String color;
    String size;
    String material;
    boolean hasHood;

    public  static void main(String args[]) {
        Jacket denimJacket = new Jacket();
        Jacket raincoat = new Jacket();

        System.out.println("denimJacket default brand: " + denimJacket.brand);
        System.out.println("denimJacket default color: " + denimJacket.color);
        System.out.println("denimJacket default size: " + denimJacket.size);
        System.out.println("denimJacket default material: " + denimJacket.material);
        System.out.println("denimJacket default has hood: " + denimJacket.hasHood);
        System.out.println();

        System.out.println("raincoat default brand: " + raincoat.brand);
        System.out.println("raincoat default color: " + raincoat.color);
        System.out.println("raincoat default size: " + raincoat.size);
        System.out.println("raincoat default material: " + raincoat.material);
        System.out.println("raincoat default has hood: " + raincoat.hasHood);
        System.out.println();

        denimJacket.brand = "Levi's";
        denimJacket.color = "Blue";
        denimJacket.size = "M";
        denimJacket.material = "Denim";
        denimJacket.hasHood = false;

        raincoat.brand = "Columbia";
        raincoat.color = "Yellow";
        raincoat.size = "L";
        raincoat.material = "Nylon";
        raincoat.hasHood = true;

        System.out.println("denimJacket updated brand: " + denimJacket.brand);
        System.out.println("denimJacket updated color: " + denimJacket.color);
        System.out.println("denimJacket updated size: " + denimJacket.size);
        System.out.println("denimJacket updated material: " + denimJacket.material);
        System.out.println("denimJacket updated has hood: " + denimJacket.hasHood);
        System.out.println();

        System.out.println("raincoat updated brand: " + raincoat.brand);
        System.out.println("raincoat updated color: " + raincoat.color);
        System.out.println("raincoat updated size: " + raincoat.size);
        System.out.println("raincoat updated material: " + raincoat.material);
        System.out.println("raincoat updated has hood: " + raincoat.hasHood);
    }
}
